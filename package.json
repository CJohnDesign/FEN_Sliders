{
  "name": "slidev-theme-FEN",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build:mf": "slidev build decks/FEN_MF/slides.md --base /mf/",
    "build:pm": "slidev build decks/FEN_PM/slides.md --base /pm/",
    "build:us": "slidev build decks/FEN_US/slides.md --base /us/",
    "dev:mf": "slidev decks/FEN_MF/slides.md --open",
    "dev:pm": "slidev decks/FEN_PM/slides.md --open",
    "dev:mc": "slidev decks/FEN_MC/slides.md --open",
    "dev:us": "slidev decks/FEN_US/slides.md --open",
    "dev:tdk": "slidev decks/FEN_TDK/slides.md --open",
    "dev:ah": "slidev decks/FEN_AH/slides.md --open",
    "dev:bwa": "slidev decks/FEN_BWA/slides.md --open",
    "dev:ghd": "slidev decks/FEN_GHD/slides.md --open",
    "dev:hcd": "slidev decks/FEN_HCD/slides.md --open",
    "dev:nce": "slidev decks/FEN_NCE/slides.md --open",
    "dev:vpp": "slidev decks/FEN_VPP/slides.md --open",
    "dev:netfh": "slidev decks/FEN_NETFH/slides.md --open",
    "dev:netmp": "slidev decks/FEN_NETMP/slides.md --open",
    "export:mf": "slidev export decks/FEN_MF/slides.md",
    "export:pm": "slidev export decks/FEN_PM/slides.md",
    "export:mc": "slidev export decks/FEN_MC/slides.md",
    "export:us": "slidev export decks/FEN_US/slides.md",
    "export:tdk": "slidev export decks/FEN_TDK/slides.md",
    "export:ah": "slidev export decks/FEN_AH/slides.md",
    "export:bwa": "slidev export decks/FEN_BWA/slides.md",
    "export:ghd": "slidev export decks/FEN_GHD/slides.md",
    "export:hcd": "slidev export decks/FEN_HCD/slides.md",
    "export:nce": "slidev export decks/FEN_NCE/slides.md",
