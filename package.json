    "export:FEN_HAO": "slidev export decks/FEN_HAO/slides.md --output exports/FEN_HAO.pdf",
    "dev:FEN_HCP": "slidev decks/FEN_HCP/slides.md",
    "export:FEN_HCP": "slidev export decks/FEN_HCP/slides.md --output exports/FEN_HCP.pdf",
    "dev:FEN_HCS": "slidev decks/FEN_HCS/slides.md",
    "export:FEN_HCS": "slidev export decks/FEN_HCS/slides.md --output exports/FEN_HCS.pdf",
    "dev:FEN_HPA": "slidev decks/FEN_HPA/slides.md",
    "export:FEN_HPA": "slidev export decks/FEN_HPA/slides.md --output exports/FEN_HPA.pdf",
    "dev:FEN_MFT": "slidev decks/FEN_MFT/slides.md",
    "export:FEN_MFT": "slidev export decks/FEN_MFT/slides.md --output exports/FEN_MFT.pdf",
    "dev:zPhase1": "slidev decks/zPhase1/slides.md",
    "export:zPhase1": "slidev export decks/zPhase1/slides.md --output exports/zPhase1.pdf"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@slidev/client": "^0.50.0-beta.7",
    "@slidev/theme-default": "^0.25.0",
    "@slidev/types": "^0.50.0-beta.5",
    "@supabase/supabase-js": "^2.75.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "dotenv": "^16.0.3",
    "elevenlabs": "^0.18.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.3.2",
    "howler": "^2.2.4",
    "node-fetch": "^3.3.2",
    "obs-websocket-js": "^5.0.6",
    "openai": "^4.76.1",
    "ora": "^8.1.1",
    "path": "^0.12.7",
    "pdf-img-convert": "^2.0.0",
    "pdf2pic": "^3.1.3",
    "simplex-noise": "^4.0.3"
  },
  "devDependencies": {
    "@slidev/cli": "^0.50.0-beta.5",
    "@types/node": "^22.10.5",
    "autoprefixer": "^10.4.20",
    "next": "^15.1.4",
    "playwright-chromium": "^1.48.2",
    "postcss": "^8.4.49",
    "puppeteer": "^19.0.0",
    "puppeteer-screen-recorder": "^3.0.6",
    "puppeteer-stream": "^3.0.22",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite-svg-loader": "^5.1.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "background": "var(--slidev-theme-background)",
      "highlighter": "shiki"
    }
  }
}
