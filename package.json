    "export-all": "node scripts/exportAll.js",
    "export-video": "node scripts/video-export/exportVideo.js",
    "youtube-upload": "node scripts/uploadToYouTube.js",
    "dev:FEN_AMER": "slidev decks/FEN_AMER/slides.md",
    "export:FEN_AMER": "slidev export decks/FEN_AMER/slides.md --output exports/FEN_AMER.pdf",
    "dev:FEN_BCR": "slidev decks/FEN_BCR/slides.md",
    "export:FEN_BCR": "slidev export decks/FEN_BCR/slides.md --output exports/FEN_BCR.pdf",
