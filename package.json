    "preview:FEN_US": "slidev decks/FEN_US/slides.md --remote",
    "build:FEN_USF": "slidev build decks/FEN_USF/slides.md --out dist/FEN_USF",
    "dev:FEN_USF": "slidev decks/FEN_USF/slides.md",
