{
  "name": "slidev-theme-FEN",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build:mf1": "slidev build decks/FEN_MF1/slides.md --base /mf1/",
    "dev:mf1": "slidev decks/FEN_MF1/slides.md --open",
    "export:mf1": "slidev export decks/FEN_MF1/slides.md",
    "screenshot": "slidev export decks/FEN_MF1/slides.md --format png",
    "generateAudio": "node scripts/generateAudio.js",
    "serve": "npx vite preview --base /mf1/"
  },
  "dependencies": {
    "@slidev/client": "^0.50.0-beta.7",
    "@slidev/theme-default": "^0.25.0",
    "@slidev/types": "^0.50.0-beta.5",
    "dotenv": "^16.0.3",
    "elevenlabs": "^0.18.0",
    "fs": "^0.0.1-security",
    "howler": "^2.2.4",
    "node-fetch": "^3.3.2",
    "ora": "^8.1.1",
    "path": "^0.12.7",
    "simplex-noise": "^4.0.3"
  },
  "devDependencies": {
    "@slidev/cli": "^0.50.0-beta.5",
    "autoprefixer": "^10.4.20",
    "playwright-chromium": "^1.48.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "vite-svg-loader": "^5.1.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "background": "var(--slidev-theme-background)",
      "highlighter": "shiki"
    }
  }
}
